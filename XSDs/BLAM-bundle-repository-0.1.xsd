<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:cmd="http://www.clarin.eu/cmd/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dcr="http://www.isocat.org/ns/dcr" xmlns:ann="http://www.clarin.eu" targetNamespace="http://www.clarin.eu/cmd/" elementFormDefault="qualified"><xs:annotation><xs:appinfo><ann:Header><ann:ID>clarin.eu:cr1:p_1475136016193</ann:ID><ann:Name>BLAM-bundle-repository-0.1</ann:Name><ann:Description>Basic Language Archive Metadata is designed to provide basic metadata for language repositories. The metadata scheme maximises discoverability and user oriented information without a proliferation of data fields and work for the data producer.</ann:Description></ann:Header></xs:appinfo></xs:annotation><xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="http://www.w3.org/2001/xml.xsd"/><xs:simpleType name="simpletype-Access-4-1---"><xs:restriction base="xs:string"><xs:enumeration value="open" dcr:datcat="" ann:label=""/><xs:enumeration value="registration required" dcr:datcat="" ann:label=""/><xs:enumeration value="request required" dcr:datcat="" ann:label=""/></xs:restriction></xs:simpleType><xs:simpleType name="Resourcetype_simple"><xs:restriction base="xs:string"><xs:enumeration value="Metadata"><xs:annotation><xs:documentation>The ResourceProxy refers to another component metadata
                            instance (e.g. for grouping metadata descriptions into
                            collections)</xs:documentation></xs:annotation></xs:enumeration><xs:enumeration value="Resource"><xs:annotation><xs:documentation>The ResourceProxy refers to a file that is not a metadata
                            instance (e.g. a text document)</xs:documentation></xs:annotation></xs:enumeration><xs:enumeration value="SearchService"><xs:annotation><xs:documentation>The ResourceProxy refers to a (SRU/CQL) web service that can be used to query the resource described in this file</xs:documentation></xs:annotation></xs:enumeration><xs:enumeration value="SearchPage"><xs:annotation><xs:documentation>The ResourceProxy refers to a web page that can be used to query the resource described in this file</xs:documentation></xs:annotation></xs:enumeration><xs:enumeration value="LandingPage"><xs:annotation><xs:documentation>The ResourceProxy refers to a web page that contains the "original context" of the resource described in this file (e.g. repository web page displaying the metadata).</xs:documentation></xs:annotation></xs:enumeration></xs:restriction></xs:simpleType><xs:complexType name="complextype-Access-4-1---"><xs:simpleContent><xs:extension base="cmd:simpletype-Access-4-1---"/></xs:simpleContent></xs:complexType><xs:element name="CMD"><xs:complexType><xs:sequence><xs:element name="Header"><xs:complexType><xs:sequence><xs:element name="MdCreator" minOccurs="0" maxOccurs="unbounded"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="MdCreationDate" minOccurs="0"><xs:complexType><xs:simpleContent><xs:extension base="xs:date"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="MdSelfLink" minOccurs="0"><xs:complexType><xs:simpleContent><xs:extension base="xs:anyURI"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="MdProfile" minOccurs="0"><xs:complexType><xs:simpleContent><xs:extension base="xs:anyURI"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="MdCollectionDisplayName" minOccurs="0"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element><xs:element name="Resources"><xs:complexType><xs:sequence><xs:element name="ResourceProxyList"><xs:complexType><xs:sequence><xs:element maxOccurs="unbounded" minOccurs="0" name="ResourceProxy"><xs:complexType><xs:sequence><xs:element maxOccurs="1" minOccurs="1" name="ResourceType"><xs:complexType><xs:simpleContent><xs:extension base="cmd:Resourcetype_simple"><xs:attribute name="mimetype" type="xs:string"/><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element maxOccurs="1" minOccurs="1" name="ResourceRef"><xs:complexType><xs:simpleContent><xs:extension base="xs:anyURI"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element></xs:sequence><xs:attribute name="id" type="xs:ID" use="required"/><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element><xs:element name="JournalFileProxyList"><xs:complexType><xs:sequence><xs:element maxOccurs="unbounded" minOccurs="0" name="JournalFileProxy"><xs:complexType><xs:sequence><xs:element maxOccurs="1" minOccurs="1" name="JournalFileRef"><xs:complexType><xs:simpleContent><xs:extension base="xs:anyURI"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element><xs:element name="ResourceRelationList"><xs:complexType><xs:sequence><xs:element maxOccurs="unbounded" minOccurs="0" name="ResourceRelation"><xs:complexType><xs:sequence><xs:element maxOccurs="1" minOccurs="1" name="RelationType"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element maxOccurs="1" minOccurs="1" name="Res1"><xs:complexType><xs:attribute name="ref" type="xs:IDREF"/><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element><xs:element maxOccurs="1" minOccurs="1" name="Res2"><xs:complexType><xs:attribute name="ref" type="xs:IDREF"/><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element><xs:element minOccurs="0" name="IsPartOfList"><xs:complexType><xs:sequence><xs:element maxOccurs="unbounded" minOccurs="0" name="IsPartOf"><xs:complexType><xs:simpleContent><xs:extension base="xs:anyURI"><xs:anyAttribute namespace="##other" processContents="lax"/></xs:extension></xs:simpleContent></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element></xs:sequence><xs:anyAttribute namespace="##other" processContents="lax"/></xs:complexType></xs:element><xs:element name="Components"><xs:complexType><xs:sequence><xs:element name="BLAM-bundle-repository-0.1" minOccurs="1" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="BundleGeneralInfo" minOccurs="1" maxOccurs="1" dcr:datcat=""><xs:complexType><xs:sequence><xs:element name="BundleID" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2573_ae7c2548-8a86-ab6e-7099-e28b7697d1a2" minOccurs="1" maxOccurs="unbounded" ann:documentation="The BundleID element contains an identifier that consistently and uniquely identifies the bundle described in the particular metadata token. The identifier should be a PID is usually generated by the repository during the ingest process." ann:displaypriority="1"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute name="identifierType"><xs:simpleType><xs:restriction base="xs:string"><xs:enumeration value="DOI" dcr:datcat="" ann:label=""/><xs:enumeration value="Handle" dcr:datcat="" ann:label=""/><xs:enumeration value="URN" dcr:datcat="" ann:label=""/><xs:enumeration value="Other" dcr:datcat="" ann:label=""/></xs:restriction></xs:simpleType></xs:attribute></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="BundleDisplayTitle" maxOccurs="unbounded" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2545_d873f2ab-2a2f-29d6-a9ab-260cde57f227" minOccurs="1" ann:documentation="The BundleDisplayTitle element provides a human readable name of the bundle. It should contain a meaningful and recognisable title for the bundle. The content of the BundleDisplayTitle element will be used as the human readable identifier in interfaces. Data producers can provide BundleDisplayTitle values for multiple interface languages."><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="BundleDescription" maxOccurs="unbounded" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2520_9eeedfb4-47d3-ddee-cfcb-99ac634bf1db" minOccurs="1" ann:documentation="The BundleDescription element provides a human readable description of the bundle. It should contain a description of the content of the collection. The content of the BundleDescription element will be used as the human readable description in interfaces. Its content can be queried by repositories for free-text metadata search. Data producers can provide BundleDescription values for multiple interface languages."><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="BundleRecordingDate" dcr:datcat="http://hdl.handle.net/11459/CCR_C-5257_6572a4ea-cc70-c721-bbd8-2f6c28cc71a1" type="xs:date" minOccurs="1" maxOccurs="1" ann:documentation="The BundleRecordingDate element contains the date at which a recording occured. The date must be provided conforming to ISO 8601 in the form YYYY-MM-DD, YYYY-MM, or YYYY."/><xs:element name="BundleKeywords" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="BundleKeyword" maxOccurs="unbounded" dcr:datcat="http://hdl.handle.net/11459/CCR_C-5436_6ab57c2c-5f8d-3561-6db6-d75da23d2637" minOccurs="1" ann:documentation="BundleKeyword should contain a single keyword or keyphrase and should be used to describe the content and nature of data to enhance the discoverability and facilitate finer granularity for searches and browsing of the data. Repositories can use keyword metadata to define meaningful subsets of the data and provide a better browsing experience." ann:displaypriority="1"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="BundleObjectLanguages" minOccurs="1" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="BundleObjectLanguage" minOccurs="1" maxOccurs="unbounded"><xs:complexType><xs:sequence><xs:element name="ObjectLanguageDisplayName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2484_669684e7-cb9e-ea96-59cb-a25fe89b9b9d" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The ObjectLanguageDisplayName element contains the name of the object language in the version recommended by the data producer. Repositories should tread the name provided by this element as the primary language name when displaying the metadata in relation to this particular data set (e.g. collection or bundle).The repository may use alternative names provided by services such as Glottolog or Ethnologue to improve discoverability and to enhance browsing and search experience. The repository may also translate the name into other languages if no language name is given for a particular interface language. Data producers can provide LanguageDisplayName values for multiple interface languages." ann:displaypriority="1"/><xs:element name="ObjectLanguageName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2484_669684e7-cb9e-ea96-59cb-a25fe89b9b9d" type="xs:string" minOccurs="0" maxOccurs="unbounded" ann:documentation="The ObjectLanguageName element contains the name of the object language in the version as provided by services such as Glottolog or Ethnologue."/><xs:element name="ObjectLanguageISO639-3Code" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2482_08eded24-4086-7e3f-88e5-e0807fb01e17" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The ObjectLanguageISO639-3 element contains an ISO 639-3 language code for the object language."/><xs:element name="ObjectLanguageGlottologCode" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The ObjectLanguageGlottologCode element contains the Glottolog code for the object language as provided by glottolog.org."/><xs:element name="ObjectLanguageLanguageFamily" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2484_669684e7-cb9e-ea96-59cb-a25fe89b9b9d" type="xs:string" minOccurs="1" maxOccurs="unbounded" ann:documentation="The ObjectLanguageLanguageFamily element contains the name of the language family and sub-families or sub-groups the object language belongs to. The values are taken from Glottolog and given in the version as provided by this service."/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="BundleLocation" minOccurs="1" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="BundleGeoLocatio" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2523_283c7b54-06ed-3d6c-4bb0-c8a79a8236fd" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The BundleGeoLocation element contains a geographical coordinates for a location point conforming to ISO 6709." ann:displaypriority="1"/><xs:element name="BundleLocationDisplayName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-5580_03e458f2-f873-8645-76eb-40e001b6c1ac" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The BundleLocationDisplayName element contains the name of a location in the version recommended by the data producer. Repositories should treat the name provided by this element as the primary location name when displaying the metadata in relation to this particular data set (e.g. collection or bundle).The repository may use alternative names provided by services such as GeoNames to improve discoverability and to enhance the browsing and search experience. The repository may also translate the name into other languages if no BundleLocationDisplayName name is given for a particular interface language. Data producers can provide BundleLocationDisplayName values for multiple interface languages."/><xs:element name="BundleLocationName" maxOccurs="unbounded" dcr:datcat="http://hdl.handle.net/11459/CCR_C-5580_03e458f2-f873-8645-76eb-40e001b6c1ac" minOccurs="0" ann:documentation="The BundleLocationName element contains the name of a location provided by services such as GeoNames. This name is used to improve discoverability and to enhance the browsing and search experience. GeoNames field: name (or toponymName)."><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="BundleRegionDisplayName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2533_fa6e1812-e29b-3cf6-e15a-50aa34b9be68" type="xs:string" minOccurs="0" maxOccurs="1" ann:documentation="The BundleRegionDisplayName element optionally contains the name of an administrative subdivision such as state, province, or any other culturally salient unit in the version recommended by the data producer. The data producer can decided which level of subdivision is relevant and will improve discoverability. Repositories should tread the name provided by this element as the primary location name when displaying the metadata in relation to this particular data set (e.g. collection or bundle). The repository may use alternative names provided by services such as GeoNames to improve discoverability and to enhance browsing and search experience. The repository may also translate the name into other languages if no RegionDisplayName value is given for a particular interface language. Data producers can provide RegionDisplayName values for multiple interface languages."/><xs:element name="BundleRegionName" maxOccurs="unbounded" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2533_fa6e1812-e29b-3cf6-e15a-50aa34b9be68" minOccurs="0" ann:documentation="The BundleRegionName element contains the name of an administrative subdivision provided by services such as GeoNames to improve discoverability and to enhance browsing and search experience. GeoNames field: adminName1"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="BundleCountryDisplayName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2532_d004b0a6-fd1d-3ca3-abf1-1e6aeb3e37b2" type="xs:string" minOccurs="0" maxOccurs="1" ann:documentation="The BundleCountryDisplayName element contains the name of the country to which the location belongs in the version recommended by the data producer. Repositories should tread the name provided by this element as the primary country name when displaying the metadata in relation to this particular data set (e.g. collection or bundle). The repository may use alternative names provided by services such as GeoNames to improve discoverability and to enhance browsing and search experience. The repository may also translate the name into other languages if no BundleCountryDisplayName name is given for a particular interface language. Data producers can provide BundleCountryDisplayName values for multiple interface languages."/><xs:element name="BundleCountryName" maxOccurs="unbounded" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2532_d004b0a6-fd1d-3ca3-abf1-1e6aeb3e37b2" minOccurs="0" ann:documentation="The BundleCountryName element contains the name of the country as provided by services such as GeoNames to improve discoverability and to enhance browsing and search experience. GeoNames field: countryName"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="BundleCountryCode" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2092_36cd7ca8-e412-9f29-7ea7-4a3ba4ba2c91" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The CountryCode element contains the ISO 3166-1 alpha 2 code of the country of the location as provided by services such as GeoNames GeoNames field: countryCode"/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="BundlePublicationInfo" minOccurs="1" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="BundlePublicationYear" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2538_8b697452-7ef3-9fce-ccf9-a7f344f11317" type="xs:gYear" minOccurs="1" maxOccurs="1" ann:documentation="BundlePublicationYear contains the year of publication in the form YYYY conforming to ISO 8601. The default value is the ingest date into the repository unless an embargo has been set for the resource. In that case the end year of the embargo is taken as the year of publication. For legacy data, the value of BundlePublicationDate can be set to a point before the ingest. The value of this field should be used to generate a bibliographical citation reference for the resource." ann:displaypriority="1"/><xs:element name="BundleDataProvider" dcr:datcat="http://hdl.handle.net/11459/CCR_C-6134_72c22724-2615-fd70-2eff-8cd3cb59e91d" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="BundleDataProvider contain the name of the data providing entity. The default value would be the name of the repository or its holding institution. The value of this field can be used to generate a bibliographical citation reference for the resource."/><xs:element name="BundleCreators" minOccurs="1" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="BundleCreator" minOccurs="1" maxOccurs="unbounded"><xs:complexType><xs:sequence><xs:element name="CreatorDisplayName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-6836_d35e73a8-ec72-d3c4-e39a-4b0fefd32cdc" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The CreatorDisplayName element contains the name of the creator in a form that lends itself to alphabetical sorting. The value of this field can be used to generate a bibliographical citation reference for the resource. This usage should guide the formatting. The data producer determines what is the proper order of names for an alphabetically sorted list." ann:displaypriority="1"/><xs:element name="CreatorNameIdentifier" dcr:datcat="" type="xs:anyURI" minOccurs="0" maxOccurs="1" ann:documentation="The CreatorNameIdentifier contains an URI that uniquely identifies the creator according to an established scheme. Best Practice: ORCID, ISNI"/><xs:element name="CreatorAffiliation" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The CreatorAffiliation contains the organisational or institutional affiliation of the creator as provided by the depositor."/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="BundleContributors" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="BundleContributor" minOccurs="1" maxOccurs="unbounded"><xs:complexType><xs:sequence><xs:element name="ContributorDisplayName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-6836_d35e73a8-ec72-d3c4-e39a-4b0fefd32cdc" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The CreatorDisplayName element contains the name of the creator. The data producer determines what is the proper order of names." ann:displaypriority="1"/><xs:element name="ContributorNameIdentifier" type="xs:anyURI" minOccurs="0" maxOccurs="1" ann:documentation="The ContributorNameIdentifier contains an URI that uniquely identifies the contributor according to an established scheme. Best Practice: ORCID, INSI"/><xs:element name="ContributorAffiliation" type="xs:string" minOccurs="0" maxOccurs="unbounded" ann:documentation="The ContributorAffiliation contains the organisational or institutional affiliation of the contributor as provided by the depositor."/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="ProjectInfo" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="Project" minOccurs="1" maxOccurs="unbounded"><xs:complexType><xs:sequence><xs:element name="ProjectDisplayName" maxOccurs="unbounded" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2536_13fc5f10-c14a-1f64-a669-32736f6d3ef5" minOccurs="1" ann:documentation="The ProjectDisplayName element provides a human readable name of the project. The preferred form is the abbreviation by which the project is generally known. The long form is best placed in the project description." ann:displaypriority="1"><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="ProjectDescription" maxOccurs="unbounded" minOccurs="1" ann:documentation="The ProjectDescription element provides a human readable description of the project in eluding its full name or title. It should contain a description of the project objective or activity. The content of the ProjectDescription element will be used as the human readable description in interfaces. Its content can be queried by repositories for free-text metadata search. Data producers can provide ProjectDescription values for multiple interface languages."><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute ref="xml:lang"/></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="FunderInfos" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="FunderInfo" minOccurs="1" maxOccurs="unbounded"><xs:complexType><xs:sequence><xs:element name="FunderName" dcr:datcat="http://hdl.handle.net/11459/CCR_C-2522_3bdc6af1-bf1b-3f5d-2938-62d99a1980ab" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The FunderName element provides the name of the funding organisation. The preferred form is the abbreviation by with the project is generally known." ann:displaypriority="1"/><xs:element name="FunderIdentifier" minOccurs="0" maxOccurs="1" ann:documentation="The FunderIdentifier contains an URI that uniquely identifies the funding body according to an established scheme."><xs:complexType><xs:simpleContent><xs:extension base="xs:string"><xs:attribute name="FunderIdentifierType"><xs:simpleType><xs:restriction base="xs:string"><xs:enumeration value="CrossrefFunder" dcr:datcat="" ann:label=""/><xs:enumeration value="ISNI" dcr:datcat="" ann:label=""/><xs:enumeration value="GRID" dcr:datcat="" ann:label=""/><xs:enumeration value="Other" dcr:datcat="" ann:label=""/></xs:restriction></xs:simpleType></xs:attribute></xs:extension></xs:simpleContent></xs:complexType></xs:element><xs:element name="GrantIdentifier" type="xs:string" minOccurs="0" maxOccurs="1" ann:documentation="GrantIdentifier contains an element that uniquely identifies the grant according to an established scheme. Best Practice: funding body specific identifier such as NSF grant number or BMBF F?rderkennzeichen."/><xs:element name="GrantURI" type="xs:anyURI" minOccurs="0" maxOccurs="1" ann:documentation="The GrantURI contains an URI that uniquely identifies the grant and funding body according to an established scheme."/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="BundleDataInfo" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="SegmentationUnits" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="SegmentationUnit" dcr:datcat="http://hdl.handle.net/11459/CCR_C-3819_833e225a-f7c9-2c41-4bd3-c55c81ef7559" type="xs:string" minOccurs="1" maxOccurs="unbounded" ann:documentation="SegmentationUnit describes the criteria applied to segment the resource. This element applies to annotations of audio-visual data and is intended to facilitate the identification of useful data for a specific research question. The best practice is to use keywords. Examples: sentence, clause, intonation unit, word" ann:displaypriority="1"/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="TranscriptionTypes" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="TranscriptionType" type="xs:string" minOccurs="1" maxOccurs="unbounded" ann:documentation="TranscriptionType describes the level of transcription of audio-visual data in the annotations. Data with no Transcription metadata will be treated as not transcribed by the repository. Examples: phonemic, phonetic, orthographic" ann:displaypriority="1"/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="TranslationLanguages" minOccurs="0" maxOccurs="1"><xs:complexType><xs:sequence><xs:element name="TranslationLanguage" minOccurs="1" maxOccurs="unbounded"><xs:complexType><xs:sequence><xs:element name="TranslationLanguageName" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The TranslationLanguageName element describes the annotation of the type translation. It contains the name of the language used in the translation. Data with no Translation metadata will be treated as not translated by the repository. Data with several Translation elements will be treated as having several translations." ann:displaypriority="1"/><xs:element name="TranslationLanguageCode" type="xs:string" minOccurs="1" maxOccurs="1" ann:documentation="The TranslationLanguageCode element describes the annotation of the type translation. It contains a language code (ISO 639-3) which identifies the language used in the translation. Data with no Translation metadata will be treated as not translated by the repository. Data with several Translation elements will be treated as having several translations."/></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element></xs:sequence><xs:attribute name="ref" type="xs:IDREFS"/></xs:complexType></xs:element><xs:element name="AnnotationTypes" minOccurs="0" m